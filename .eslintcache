put>\r\n            <Icon.Social icon={icon} bgColor={bgColor}/>\r\n            <InputText {...rest}/>\r\n        </StyledIconInput>\r\n    );\r\n}\r\n\r\n\r\nEditProfile.propTypes = {\r\n    children: PropTypes.any\r\n};\r\n\r\nexport default EditProfile;","C:\\Users\\14165\\Desktop\\components\\src\\stories\\editProfile.stories.js",["623"],"import React from \"react\";\r\nimport EditProfile from \"../components/EditProfile\"\r\nimport  \"stories/story.css\"\r\n\r\nexport default {\r\n    title: \"页面/EditProfile\",\r\n    components: EditProfile\r\n};\r\n\r\nexport const Default = () => <EditProfile/>","C:\\Users\\14165\\Desktop\\components\\src\\components\\Settings\\index.js",["624"],"import React from 'react';\r\nimport StyleSettings, {SettingsItemControl, StyledSettingsGroup, StyledSettingsItem} from \"./style\";\r\nimport PropTypes, {func} from 'prop-types';\r\nimport Paragraph from \"../Paragraph\";\r\nimport Switch from \"../Switch\";\r\nimport Icon from \"../Icon\";\r\nimport {ReactComponent as ArrowMenuRight} from \"../../asserts/icons/arrowMenuRight.svg\"\r\nimport Seperator from \"../Seperator\";\r\nimport {Link} from \"react-router-dom\";\r\nimport \"styled-components/macro\"\r\nimport {useSpring, animated} from \"react-spring\";\r\n\r\nfunction Settings({children, ...rest}) {\r\n\r\n    const animeProps = useSpring({\r\n        transform: \"translate3d(0px, 0px, 0px)\",\r\n        opacity: 1,\r\n        from: {transform: \"translate3d(100px, 0px, 0px)\", opacity: 0},\r\n        config: {\r\n            tension: 140,\r\n        },\r\n        delay: 300,\r\n    })\r\n\r\n\r\n    return (\r\n        <StyleSettings {...rest}>\r\n            <animated.div style={animeProps}>\r\n                <SettingsGroup groupName={\"隐私设置\"}>\r\n                    <SettingsItem label={\"添加好友时需要验证\"}/>\r\n                    <SettingsItem\r\n                        label={\"推荐通讯录好友\"}\r\n                        description={\"上传的通讯录只用来匹配好友列表，本应用不会记录和发送任何信息给其他机构或组织\"}\r\n                    />\r\n                    <SettingsItem label={\"只能通过手机号找到我\"}/>\r\n                </SettingsGroup>\r\n\r\n\r\n                <SettingsGroup groupName={\"通知设置\"}>\r\n                    <SettingsItem label={\"新消息通知\"}/>\r\n                    <SettingsItem label={\"语音和视频童话提醒\"}/>\r\n                    <SettingsItem label={\"显示通知详情\"}/>\r\n                    <SettingsItem label={\"声音\"}/>\r\n                    <Link to={\"/settings/blocked\"}\r\n                          css={`\r\n                        text-decoration: none; \r\n                        color: inherit`\r\n                          }>\r\n                        <SettingsItem label={\"查看已静音的好友列表\"} type={\"menu\"}/>\r\n                    </Link>\r\n\r\n                </SettingsGroup>\r\n            </animated.div>\r\n\r\n\r\n        </StyleSettings>\r\n    );\r\n}\r\n\r\nfunction SettingsGroup({groupName, children, ...rest}) {\r\n    return (\r\n        <StyledSettingsGroup>\r\n            <Paragraph size={\"xxlarge\"} style={{paddingBottom: \"24px\"}}>\r\n                {groupName}\r\n            </Paragraph>\r\n            {children}\r\n        </StyledSettingsGroup>\r\n    )\r\n}\r\n\r\nexport function SettingsItem(\r\n    {\r\n        type = \"switch\",\r\n        label,\r\n        description,\r\n        children,\r\n        ...rest\r\n\r\n    }) {\r\n    return (\r\n        <StyledSettingsItem {...rest} >\r\n            <SettingsItemControl>\r\n                <Paragraph size={\"large\"}>{label}</Paragraph>\r\n                {type === \"switch\" && <Switch/>}\r\n                {type === \"menu\" && <Icon icon={ArrowMenuRight}/>}\r\n            </SettingsItemControl>\r\n\r\n            {description && (\r\n                <Paragraph type={\"secondary\"} style={{margin: \"4px 0\"}}>\r\n                    {description}\r\n                </Paragraph>\r\n            )}\r\n\r\n            <Seperator style={{marginTop: \"8px\", marginBottom: \"20px\"}}/>\r\n\r\n        </StyledSettingsItem>\r\n\r\n\r\n    )\r\n\r\n}\r\n\r\n\r\nSettings.propTypes = {\r\n    children: PropTypes.any,\r\n    label: PropTypes.string,\r\n    description: PropTypes.string,\r\n    type: PropTypes.string\r\n};\r\n\r\nexport default Settings;\r\n","C:\\Users\\14165\\Desktop\\components\\src\\stories\\settings.stories.js",["625"],"import React from \"react\";\r\nimport Settings, {SettingsItem} from \"../components/Settings\"\r\nimport  \"stories/story.css\"\r\n\r\nexport default {\r\n    title: \"页面/Settings\",\r\n    components: Settings\r\n};\r\n\r\nexport const Default = () => <Settings/>\r\n\r\nexport const WithoutDescription = () => (\r\n    <SettingsItem label={\"这是一个没有描述的设置项\"}/>\r\n\r\n    )\r\n\r\nexport const WithDescription = () => (\r\n    <SettingsItem label={\"这是一个有描述的设置项\"} description={\"这是设置项描述\"}/>\r\n\r\n)\r\n\r\nexport const WithMenu = () => (\r\n    <SettingsItem label={\"有子菜单的设置项\"} type={\"menu\"}/>\r\n)\r\n\r\n","C:\\Users\\14165\\Desktop\\components\\src\\components\\Settings\\style.js",[],"C:\\Users\\14165\\Desktop\\components\\src\\stories\\blockedList.stories.js",["626"],"import React from \"react\";\r\nimport BlockedList from \"../components/BlockedList\"\r\nimport  \"stories/story.css\"\r\n\r\nexport default {\r\n    title: \"页面/BlockedList\",\r\n    components: BlockedList\r\n};\r\n\r\nexport const Default = () => <BlockedList/>","C:\\Users\\14165\\Desktop\\components\\src\\components\\BlockedList\\style.js",[],"C:\\Users\\14165\\Desktop\\components\\src\\components\\BlockedList\\index.js",["627"],"import React from 'react';\r\nimport StyleBlockedList, {\r\n    BlockedAvatar,\r\n    BlockedName,\r\n    ClosableAvatar,\r\n    CloseIcon,\r\n    FriendList,\r\n    SettingsMenu\r\n} from \"./style\";\r\nimport PropTypes from 'prop-types';\r\nimport Icon from \"../Icon\";\r\nimport {ReactComponent as ArrowMenuLeft} from \"../../asserts/icons/arrowMenuLeft.svg\";\r\nimport {ReactComponent as closeCircle} from \"../../asserts/icons/cl